package friday.geometryfigure.point;

public final class Rectangle {
	private Point p1;
	private Point p2;

	public Rectangle(Point p1, Point p2) {
		if (p1.equals(p2)) {
			System.out.println("Points are on the same axis.");
		} else {
			this.p1 = p1;
			this.p2 = p2;
		}
	}

	public Rectangle(Rectangle rect) {
		this(rect.p1, rect.p2);
	}

}
