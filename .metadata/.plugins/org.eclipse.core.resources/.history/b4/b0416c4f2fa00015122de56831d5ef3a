package friday.geometryfigure.point;

import friday.geometryfigure.GeometryFigures;

public class Point  {
	private int x;
	private int y;

	public Point() {
		x = 0;
		y = 0;
	}

	public Point(int x, int y) {
		this.x = x;
		this.y = y;
	}

	public Point(Point point) {
		this(point.x, point.y);
	}	
}
