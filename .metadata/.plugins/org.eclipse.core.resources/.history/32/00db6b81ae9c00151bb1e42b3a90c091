package friday.todolist.task;

public class LearnGeometryTask extends Task {
	public LearnGeometryTask() {
		super.timeNeeded = 1;
	}

	public LearnGeometryTask(float time) {
		super.timeNeeded = time;
	}

	@Override
	public String toString() {
		return "LearnGeometryTask [timeNeeded=" + timeNeeded + "]";
	}
}
