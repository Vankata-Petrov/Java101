package friday.todolist.task;

public class StudyForAlgebraTask extends Task {
	public StudyForAlgebraTask() {
		super.priority = 0;
		super.timeNeeded = 0;
	}

	public StudyForAlgebraTask(int priority, float time) {
		super.priority = priority;
		super.timeNeeded = time;
	}

	@Override
	public String toString() {
		return "StudyForAlgebraTask [ time needed: "
				+ String.valueOf(super.getTimeNeeded()) + " priority: "
				+ super.getPriority() + " ]";
	}
}
