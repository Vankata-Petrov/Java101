			CurrencyCourse.AUSTRALIAN_DOLLAR);
	private int vat;
	private int code;
	private boolean isDefault;
	private CurrencyCourse currency;

	private Country(int vat, boolean isDefault, CurrencyCourse currency) {
		this.vat = vat;
		this.isDefault = isDefault;
		this.currency = currency;
	}

	public int getVat() {
		return vat;
	}

	public boolean isDefault() {
		return isDefault;
	}

	public CurrencyCourse getCurrency() {
		return currency;
	}
