package friday.todolist.task;

public class GoOutTask extends Task {

	public GoOutTask() {
		this.priority = 0;
		this.timeNeeded = 0;
	}

	public GoOutTask(float time) {
		super.timeNeeded = time;
		super.priority = 0;
	}

	@Override
	public String toString() {
		return "Go out Task [ time needed: "
				+ String.valueOf(this.getTimeNeeded()) + " priority: "
				+ this.getPriority() + " ]";
	}
}
