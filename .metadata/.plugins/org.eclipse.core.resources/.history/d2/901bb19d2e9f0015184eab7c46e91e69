package friday.hashmap;

public class HashMapTest {
	public static void main(String[] args) {
		MyHashMap<String, String> myHashMap = new MyHashMap<>();
		myHashMap.put("Sofia", "Pesho");
		myHashMap.put("Bansko", "Gosho");
		myHashMap.put("Varna", "Pesho");
		myHashMap.put("Pleven", "Minko");
		myHashMap.put("Burgas", "Boyan");
		myHashMap.put("Vraca", "Ginka");
		myHashMap.put("NUll", null);
		myHashMap.put("Pirdop", "Pesho");
		myHashMap.put("Zlatica", "Gosho");
		myHashMap.put("Samokov", "Pesho");
		myHashMap.put("Razgrad", "Minko");
		//System.out.println(myHashMap);
		myHashMap.put("Plovdiv", "Boyan");
		//System.out.println(myHashMap);
		myHashMap.put("Lukovit", "Ginka");
		//System.out.println(myHashMap);
		myHashMap.put("Ruse", "Pesho");
		//System.out.println(myHashMap);
		myHashMap.put("Vidin", "Gosho");
		myHashMap.put("London", "Pesho");
		myHashMap.put("Berlin", "Minko");
		myHashMap.put("Madrid", "Boyan");
		myHashMap.put("Paris", "Ginka");
		//System.out.println(myHashMap.remove("Sofia"));
		System.out.println(myHashMap);
		System.out.println(myHashMap.containsKey("Sofia"));
		System.out.println(myHashMap.values());
	}
}
