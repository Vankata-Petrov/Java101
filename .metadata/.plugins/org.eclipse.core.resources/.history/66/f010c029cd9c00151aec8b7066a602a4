package friday.todolist.task;

public class GoOutTask extends Task {

	public GoOutTask() {
		super.priority = 0;
		super.timeNeeded = 0;
	}

	public GoOutTask(int priority, float time) {
		super.timeNeeded = time;
		super.priority = priority;
	}

	@Override
	public String toString() {
		return "Go out Task [ time needed: "
				+ String.valueOf(super.getTimeNeeded()) + " priority: "
				+ super.getPriority() + " ]";
	}
}
