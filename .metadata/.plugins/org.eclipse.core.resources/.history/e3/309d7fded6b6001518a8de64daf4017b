	public double withdraw(BankAccount account, double money) {
		// TODO Auto-generated method stub
		return 0;
	}

	@Override
	public boolean transfer(BankAccount account1, BankAccount account2,
			double money) {
		// TODO Auto-generated method stub
		return false;
	}

}
