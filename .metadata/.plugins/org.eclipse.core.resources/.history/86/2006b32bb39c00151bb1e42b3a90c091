package friday.todolist.factory;

import friday.todolist.task.StudyForAlgebraTask;
import friday.todolist.task.Task;

public class TaskFactory {
	public static Task createTask(String taskName) {
		switch (taskName) {
		case "algebra":
			return new StudyForAlgebraTask();
		default:
			break;
		}
		return null;
	}
}
